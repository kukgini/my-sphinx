bosh create-env $BOSH_DEPLOYMENT/bosh.yml \
-o $BOSH_DEPLOYMENT/openstack/cpi.yml \
-o $BOSH_DEPLOYMENT/external-ip-with-registry-not-recommended.yml \
--state $PWD/state.json \
--vars-store $PWD/creds.yml \
--var-file private_key=$PRIVATE_KEY \
-v default_key_name=$KEY_NAME \
-v auth_url="$AUTH_URL" \
-v openstack_username="$USERNAME" \
-v openstack_password="$PASSWORD" \
-v openstack_domain="$DOMAIN" \
-v openstack_project="$PROJECT" \
-v region=$REGION \
-v az=$AZ \
-v default_security_groups=[bosh] \
-v director_name=$DIRECTOR_NAME \
-v net_id=$NET_ID \
-v internal_cidr=$INTERNAL_CIDR \
-v internal_gw=$INTERNAL_GW \
-v internal_ip=$INTERNAL_IP \
-v external_ip=$EXTERNAL_IP